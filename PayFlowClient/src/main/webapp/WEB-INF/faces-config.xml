		<managed-bean-class>com.java.jsf.daoImpl.UseDaoImpl
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>



	<!-- Group model -->
	<managed-bean>
		<managed-bean-name>group</managed-bean-name>
		<managed-bean-class>com.java.jsf.model.Group</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<!-- User model -->
	<managed-bean>
