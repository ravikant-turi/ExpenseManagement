		<managed-bean-class>com.java.jsf.daoImpl.UseDaoImpl
		</managed-bean-class>
		<managed-bean-scope>session</managed-bean-scope>
	</managed-bean>

	<!-- GroupMemberDao -->
	<managed-bean>
